<div class="row">
  <h1>Your Tracking Data</h1>
  <div class="accordian" id="sites">
    <% @current_user.sites.each do |site| %>
    <div class="accordian-group">
      <div class="accordion-heading">
        <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#sites" href="#collapse"><h1><%= site.name %></h1></a>
      </div>
      <div class="accordian-body collapse">
        <div class="accordian-inner">
          <table class="table table-striped table-bordered table-hover">
            <tr>
              <th>Time</th>
              <th>Url</th>
              <th>Event</th>
              <th>Data</th>
              <th>IP Address</th>
              <th>Location</th>
            </tr>
            <% site.visits.each do |visit| %>
            <tr>
              <td><%= visit.created_at %></td>
              <td><%= visit.url %></td>
              <td><%= visit.event %></td>
              <td><%= visit.data %></td>
              <td><%= visit.ip_address %></td>
              <td><%= visit.location %></td>
            </tr>
            <% end %>
          </table>
        </div>
      </div>
    </div>
    <% end %>
